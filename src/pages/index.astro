---
import BaseLayout from '../layouts/BaseLayout.astro'
import Power from '../components/power'
const items = ['gobs', 'ligdh', 'mitches']
const visible = true
---

<BaseLayout>
    <p>Teste</p>
    {
        items.map( item => (
            visible && <p>{item}</p>
        ) )
    }
    <Power nome='teste' startCounter={3}></Power>
</BaseLayout>
